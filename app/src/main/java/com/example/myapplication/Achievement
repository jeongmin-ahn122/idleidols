package com.example.myapplication;  // Package and imports copy-pasted from Achievements

public class Achievement
{
    // variables
    public String name, description;
    public Image image;
    public boolean claimed, unlocked;
    public int exp, money, seeds;

    //
    // Accessor methods
    //
    boolean isUnlocked() {return unlocked;}

    boolean isClaimed() {return claimed;}

}
